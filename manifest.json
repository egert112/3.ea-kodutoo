{
    "name": "Avaja",
    "description": "Avab mitu tabi sinu soovil",
    "version": "1.3.3.7",
    "permissions": ["<all_urls>", "tabs"],

    "browser_action": {
      "default_popup": "popup.html"
    },

    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["options.js"]
      }],
    "manifest_version": 2
  }